        "showHeader": true,
        "sortBy": [
          {
            "desc": false,
            "displayName": "Marketplace"
          }
        ]
      },
      "pluginVersion": "12.0.2",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "editorMode": "code",
          "expr": "# Available parser data\n(dealhunter_parser_health_status{environment=~\"$environment\"} * 0 + 1) or \n# Generate missing marketplaces with zero values \nlabel_replace(\n  label_replace(\n    label_replace(\n      vector(0),\n      \"marketplace\", \"facebook\", \".*\", \".*\"\n    ),\n    \"marketplace\", \"ebay\", \".*\", \".*\"\n  ),\n  \"marketplace\", \"craigslist\", \".*\", \".*\"\n)",
          "format": "table",
          "legendFormat": "",
          "range": false,
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Marketplace Parser Data Status Matrix",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "environment": true,
              "instance": true,
              "job": true
            },
            "indexByName": {},
            "renameByName": {
              "Value": "Status",
              "marketplace": "Marketplace"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Status",
            "binary": {
              "left": "Status",
              "operator": ">",
              "reducer": "sum",
              "right": "0"
            },
            "mode": "binary",
            "reduce": {
              "include": ["Status"],
              "reducer": "sum"
            },
            "replaceFields": true
          }
        },
        {
          "id": "fieldToConfigMapping",
          "options": {
            "mappings": {
              "Status": {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "true": {
                        "index": 0,
                        "text": "DATA AVAILABLE"
                      }
                    },
                    "type": "value"
                  },
                  {
                    "options": {
                      "false": {
                        "index": 1,
                        "text": "MISSING DATA"
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            }
          }
        }
      ],
      "type": "table"
    },
