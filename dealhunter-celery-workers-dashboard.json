{
  "title": "DealHunter - Celery Workers & Task Processing",
  "uid": "dealhunter-celery-workers",
  "tags": [
    "dealhunter",
    "celery",
    "workers",
    "tasks"
  ],
  "timezone": "browser",
  "schemaVersion": 27,
  "version": 0,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "type": "timeseries",
      "title": "Task Creation Rate (per minute)",
      "targets": [
        {
          "expr": "sum(rate(dealhunter_tasks_created_total[5m])) by (task_type) * 60",
          "legendFormat": "{{task_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Tasks/min",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never",
            "spanNulls": true
          },
          "unit": "ops",
          "min": 0
        }
      }
    },
    {
      "id": 2,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "type": "timeseries",
      "title": "Task Completion Rate (per minute)",
      "targets": [
        {
          "expr": "sum(rate(dealhunter_tasks_completed_total[5m])) by (task_type, result) * 60",
          "legendFormat": "{{task_type}} - {{result}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Tasks/min",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never"
          },
          "unit": "ops",
          "min": 0
        }
      }
    },
    {
      "id": 3,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "type": "timeseries",
      "title": "Queue Lengths by Type",
      "targets": [
        {
          "expr": "dealhunter_task_queue_length",
          "legendFormat": "{{queue_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Queue Depth",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "none",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1000,
                "color": "yellow"
              },
              {
                "value": 5000,
                "color": "red"
              }
            ]
          },
          "unit": "short",
          "min": 0
        }
      }
    },
    {
      "id": 4,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "type": "stat",
      "title": "Total Queue Depth",
      "targets": [
        {
          "expr": "sum(dealhunter_task_queue_length)",
          "legendFormat": "Total Tasks in Queue",
          "instant": true,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 5000,
                "color": "yellow"
              },
              {
                "value": 10000,
                "color": "red"
              }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area"
      }
    },
    {
      "id": 5,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "type": "timeseries",
      "title": "Task Execution Duration (p50, p95, p99)",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(dealhunter_task_execution_duration_seconds_bucket[5m])) by (le, task_type))",
          "legendFormat": "{{task_type}} p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(dealhunter_task_execution_duration_seconds_bucket[5m])) by (le, task_type))",
          "legendFormat": "{{task_type}} p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(dealhunter_task_execution_duration_seconds_bucket[5m])) by (le, task_type))",
          "legendFormat": "{{task_type}} p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Duration",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 0,
            "gradientMode": "none",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never"
          },
          "unit": "s",
          "min": 0
        }
      }
    },
    {
      "id": 6,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      },
      "type": "timeseries",
      "title": "Task Success Rate",
      "targets": [
        {
          "expr": "sum(rate(dealhunter_tasks_completed_total{result=\"success\"}[5m])) by (task_type) / sum(rate(dealhunter_tasks_completed_total[5m])) by (task_type) * 100",
          "legendFormat": "{{task_type}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Success %",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 80,
                "color": "yellow"
              },
              {
                "value": 95,
                "color": "green"
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 7,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      },
      "type": "timeseries",
      "title": "Active Queue Processors",
      "targets": [
        {
          "expr": "dealhunter_queue_processor_marketplaces_active",
          "legendFormat": "Active Marketplaces",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Active Count",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 20,
            "gradientMode": "none",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never"
          },
          "unit": "short",
          "min": 0,
          "max": 6
        }
      }
    },
    {
      "id": 8,
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      },
      "type": "timeseries",
      "title": "Items Processed (Total)",
      "targets": [
        {
          "expr": "sum(rate(dealhunter_queue_processor_items_processed_total[5m])) * 60",
          "legendFormat": "Items/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "Items/min",
            "axisPlacement": "auto",
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "showPoints": "never"
          },
          "unit": "ops",
          "min": 0
        }
      }
    }
  ]
}
